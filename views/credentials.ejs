<!doctype html>
<html>

<head>
	<!--[if lt IE 9]>
	    <script src="bower_components/html5shiv/dist/html5shiv.js"></script>
	<![endif]-->
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<link rel="stylesheet" href="bootstrap/dist/css/bootstrap.css">
	<link rel="stylesheet" href="main.css">
	<title>Cusor Game</title>
</head>
<body>
	<div class="center_float">
		<form id="register" class="form-group">
			<h2>Register</h2>
			<label>Email:</label>
			<input id="email" class="form-control"  placeholder="Please Enter Your Email"/>
			<br/>
			<label>Password:</label>
			<input id="password" class="form-control" type="password" placeholder="Please Enter Your Passord"/>
			<a href="#" id="login_link" class="help-block">Already Registered? Login here</a>
			<button id="submit_register" type="submit" class="btn btn-default" >Submit</button>
		    <div id="reg_prob" class="alert has-error">
		        <a href="#" class="close" data-dismiss="alert">&times;</a>
		        <strong>Error!</strong> <span></span>
		    </div>
		</form>
		<form id="login" class="form-group">
			<h2>Login</h2>
			<label>Email:</label>
			<input id="email_login" class="form-control" placeholder="Please Enter Your Email"/>
			<br/>
			<label>Password:</label>
			<input id="password_login" class="form-control" type="password" placeholder="Please Enter Your Passord"/>
			<a href="#" id="register_link" class="help-block">Need an account? Register here</a>
			<button id="submit_login" type="submit" class="btn btn-default" >Submit</button>
	        <div id="log_prob" class="alert has-error">
		        <a href="#" class="close" data-dismiss="alert">&times;</a>
		        <strong>Error!</strong> <span></span>
		    </div>
		</form>
	</div>
</body>
<script src="jquery/dist/jquery.js"></script>
<script src="bootstrap/dist/js/bootstrap.js"></script>
<script src="main.js"></script>
</html>